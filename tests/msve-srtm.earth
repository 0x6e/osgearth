<map name="MSVE" type="geocentric">

    <projection>plate carre</projection>
    <min_tile_range_factor>8</min_tile_range_factor>
    <vertical_scale>4</vertical_scale>
    
<!-- uncomment to activate caching: -->    
<!-- <cache_path>f:/temp/osgearthcache</cache_path> -->
    
<!-- uncomment to activate a proxy server: -->
<!-- <proxy_host>80.80.12.123</proxy_host> -->
<!-- <proxy_port>80</proxy_port> -->

   <image name="MSVE Public" driver="msve">
       <url>http://h0.ortho.tiles.virtualearth.net/tiles</url>
       <dataset>hybrid</dataset>
   </image>
   
   <heightfield name="JPL SRTM Plus" driver="wms">
        <url>http://onearth.jpl.nasa.gov/wms.cgi</url>
        <layers>worldwind_dem</layers>
        <style>short_int</style>
        <format>png</format>
        <tile_width>8</tile_width>
        <tile_height>8</tile_height>
   </heightfield>
</map>

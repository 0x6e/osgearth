<map name="Google with labels" type="geocentric">

    <projection>mercator</projection>
    <min_tile_range_factor>8</min_tile_range_factor>

  <!-- uncomment the following line to enable caching... folder must exist -->
  <!-- <cache_path>f:/temp/osgearthcache</cache_path> -->
    
<!-- uncomment the following two lines to activate a proxy server -->
<!-- <proxy_host>80.80.12.123</proxy_host> -->
<!-- <proxy_port>80</proxy_port> -->

  <image name="Google Maps" driver="google">
    <!-- datasets: satellite, terrain, labels -->
    <dataset>satellite</dataset>
  </image>

  <image name="Google Labels" driver="google">
    <!-- datasets: satellite, terrain, labels -->
    <dataset>labels</dataset>
  </image>
  
  <!-- Polar caps to match..  -->
  <north_cap_color>0 0 0 255</north_cap_color>
  <south_cap_color>255 255 255 255</south_cap_color>
   
</map>

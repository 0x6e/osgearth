<!--
osgEarth Sample

Demonstrates the use of the Google driver to build a terrain model using road
map tiles from the Google Maps service.

This is for educational purposes only - it is your responsibility to abide by
the provider's terms of service when using their data.
-->

<map name="Google" type="geocentric">

    <min_tile_range_factor>8</min_tile_range_factor>
    
    <image name="Google maps" driver="google">
        <dataset>roads</dataset>
    </image>
   
    <!-- Matching cap colors -->
    <north_cap_color>153 179 204 255</north_cap_color>
    <south_cap_color>255 255 255 255</south_cap_color>
   
</map>
